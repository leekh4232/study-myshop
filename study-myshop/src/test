    class ClassName1 {
        +ClassName1(param1: Type1)
        +method1(param2: Type2): ReturnType1
    }

    class ClassName2 {
        +ClassName2(param1: Type1)
        +method2(param2: Type2): ReturnType2
    }

    class ClassName3 {
        +ClassName3(param1: Type1)
        +method3(param2: Type2): ReturnType3
    }

    ClassName1 --|> ClassName2 : IsA
